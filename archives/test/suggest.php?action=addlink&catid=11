               <a {% if (file.url) { %}href="{%=file.url%}"{% } %} title="{%=file.name%}" download="{%=file.name%}" {%=file.thumbnailUrl?'data-gallery':''%}>{%=file.name%}</a>            
            {% if (file.error) { %}
                <div><strong class="error">{%=file.error%}</strong></div>
